{
    "is_prj_leader" : {
        "name" : "prj_info",
        "type" : "call",
        "method" : "GET",
        "tokenSign" : "APP",
        "cache" : "#PID#",
        "comment" : "查询帐号在项目中的角色",
        "service" : "iproject",
        "url" : "/role?id=#PID#&account=@{#tokenAcc}",
        "afterAll" : "
            if('@{!role}'!='L') {
                Mesh.error(RetCode.NO_RIGHT, 'not leader');
            } else {
                Mesh.success({});
            }
        "
    },
    
    "is_prj_member" : {
        "name" : "prj_info",
        "type" : "call",
        "method" : "GET",
        "tokenSign" : "APP",
        "cache" : "#PID#",
        "comment" : "确认是否为项目成员",
        "service" : "iproject",
        "url" : "/role?id=#PID#&account=@{#tokenAcc}",
        "afterAll" : "
            if('@{!role}'=='') {
                Mesh.error(RetCode.NO_RIGHT, 'not a member');
            } else {
                Mesh.success({});
            }
        "
    }
}