{
    "is_prj_leader" : {
        "name" : "prj_info",
        "type" : "call",
        "method" : "GET",
        "tokenSign" : "APP",
        "cache" : "#PID#",
        "comment" : "查询帐号在项目中的角色",
        "service" : "iproject",
        "url" : "/role?id=#PID#&account=@{#tokenAcc}",
        "onSuccess" : {
            "errorCode":"NO_RIGHT",
            "errorInfo":"not leader",
            "condition":"@{CONDITION|!role,'s.!=','L'}"
        }
    },
    
    "is_prj_member" : {
        "name" : "prj_info",
        "type" : "call",
        "method" : "GET",
        "tokenSign" : "APP",
        "cache" : "#PID#",
        "comment" : "确认是否为项目成员",
        "service" : "iproject",
        "url" : "/role?id=#PID#&account=@{#tokenAcc}",
        "onSuccess" : {
            "errorCode":"NO_RIGHT",
            "errorInfo":"not a member",
            "condition":"@{CONDITION|!role,'s.==',''}"
        }
    },
    
    "judge_factory_right" : {
        "name" : "judge_factory_right",
        "type" : "dataexists",
        "db":"common",
        "expect" : true,
        "errorCode":"NO_RIGHT",
        "errorInfo":"not a valid member",
        "sql":"select * from factoryAdm 
            where acc='@{#tokenAcc}' and factory=#FACTORY#
            and type in(#TYPE#)"
    },

    "check_accounts":{
        "name":"check_accounts",
        "comment":"检查帐号是否都存在",
        "type" : "call",
        "service": "user",
        "method":"POST",
        "url":"/user/userid",
        "tokenSign":"APP",
        "parameters":"{\"accounts\":#ACCLIST#}"
    }
}